[ignore]
.*/api/node_modules/.*
.*/dist-server/.*
./bin
./scripts
./customer-ui/.*

[include]
./shared-lib
./admin-ui

[libs]
./flow/declarations/

[lints]

[options]
module.file_ext=.js
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.svg
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper.extension='png' -> 'empty/object'
module.name_mapper.extension='jpg' -> 'empty/object'
module.name_mapper.extension='svg' -> 'empty/object'
module.name_mapper='^\@flow\/\(.*\)$' -> '<PROJECT_ROOT>/flow/declarations/\1'
module.name_mapper='^\@shared\/\(.*\)$' -> '<PROJECT_ROOT>/src/shared/\1'
module.name_mapper='^\@translations\/\(.*\)$' -> '<PROJECT_ROOT>/src/translations/\1'
module.name_mapper='^\@src\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^\@constants\/\(.*\)$' -> '<PROJECT_ROOT>/src/constants/\1'
module.name_mapper='^\@components\/\(.*\)$' -> '<PROJECT_ROOT>/src/shared/components/\1'
module.name_mapper='^\@lib\/\(.*\)$' -> '<PROJECT_ROOT>/src/shared/lib/\1'
sharedmemory.hash_table_pow=21